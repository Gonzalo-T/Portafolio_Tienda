<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    <script src="https://code.jquery.com/jquery-3.6.0.js"
            integrity="sha256-H+K7U5CnXl1h5ywQfKtSj8PCmoN9aaq30gDh27Xc0jk="
            crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.0/jquery.validate.min.js"></script>

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>



    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>

<body>
    <div class="header">
        <div class="welcome">
            <h6>
                <span class="nav-link efecto " style="letter-spacing: 8px; font-size: x-large; padding-top: 3px; padding-bottom: 3px;">@Html.ActionLink("TALLER UNIQUE", "Index", "Home", null, new { @class = "efecto", @style = "text-decoration:none; " }) </span>
            </h6>
        </div>
        <div class="state">
       

                <a href='@Url.Action("Index", "Home")'>
                    <img src='@Url.Content("~/Content/img/Volver.png")' style="width:190px; height: 40px; padding-left: 150px; white-space: nowrap;" />
                </a>

                <span class="nav-link Normal " href="" style="font-size: calc(1vw); padding-left: 130px; white-space: nowrap; ">Volver a inicio</span>


        </div>

    </div>

    <div class="main">
        <!--    MENÚ    -->
        <nav class="navbar navbar-expand-lg navbar-light bg-dark ">
            <div class="container-fluid">
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
                        aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarSupportedContent" style="font-size:larger; width: 100%; border-color: black;">
                    <ul class="navbar-nav me-auto mb-2 mb-lg-0">

                        <li class="nav-item p-3">
                            <span class="nav-link Normal " style=" padding-left: 20px; font-size: large; white-space: nowrap; padding-top: 13px">@Html.ActionLink("Nuestros Productos", "IndexProductos", "Productos", null, new { @class = "Normal", @style = "text-decoration:none; " })</span>
                        </li>

                        <li class="nav-item p-3">
                            <span class="nav-link Normal " style=" padding-left: 20px; font-size: large; white-space: nowrap; padding-top: 13px">@Html.ActionLink("Confecciones", "IndexProductos", "Productos", null, new { @class = "Normal", @style = "text-decoration:none; " })</span>
                        </li>

                        <li class="nav-item p-3">
                            <span class="nav-link Normal " style=" padding-left: 20px; font-size: large; white-space: nowrap; padding-top: 13px">@Html.ActionLink("Contactenos", "ContactenosNoRegistrado", "Contacto", null, new { @class = "Normal", @style = "text-decoration:none; " })</span>
                        </li>



                    </ul>
                    <!--
                    <form class="d-flex">
                      <input class="form-control me-2" type="Buscar" placeholder="Buscar" aria-label="Buscar">
                      <button class="btn btn-outline-success" type="submit">Búsqueda</button>
                    </form>
                    -->
                </div>


            </div>
        </nav>
        <!--
        <div class="container" style="margin-top: 100px;" style="color: black;">
          {% block content %}

          {% endblock %}
        </div>
        -->

        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - Tu mundo de confección, powered by ClothesData</p>
            </footer>
        </div>

        @RenderSection("scripts", required: false)


    </div>


    <div class="back">
        <header style="display:block">

            <nav class="navbar navbar-expand-md navbar-light contenedor" style="background-color:rgb(55 65 81); width: 100%; ">
                <div class="container-fluid">
                    <button class="navbar-toggler contenedor" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
                            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation" style="background-color:white">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse contenedor " id="navbarSupportedContent" style=" background-color: rgb(55 65 81); width: 100% ">
                        <ul class="navbar-nav me-auto mb-2 mb-lg-0 contenedor" style="height: 100%; width: 100%; padding-top: 15px; padding-left: 15px">
                            <li class="nav-item ">
                                <span class="nav-link efecto " href="" style="letter-spacing: 8px; font-size: x-large; ">@Html.ActionLink("CLOTHES DATA", "Index", "Home", null, new { @class = "efecto", @style = "text-decoration:none; " }) </span>

                            </li>

                            <li class="nav-item">
                                <a class="nav-link popover-test" href="#" style="color: rgb(55 65 81); font-size: large; border-radius: 1%; border-image: initial; background-color: white; ">Bienvenido a ClothesDada</a>
                            </li>
                            <!--<li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown"
                                   aria-expanded="false" style="color: white">
                                    Trabajos por Mecánico
                                </a>
